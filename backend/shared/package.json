{
  "name": "@phishing-detection/shared",
  "version": "1.0.0",
  "description": "Shared utilities and types for Real-Time-AI-ML-Based-Phishing-Detection-and-Prevention-System",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d dist/database/data-source.js",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d dist/database/data-source.js",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d dist/database/data-source.js",
    "migration:show": "typeorm-ts-node-commonjs migration:show -d dist/database/data-source.js",
    "seed": "ts-node -r tsconfig-paths/register database/seed.ts",
    "verify": "ts-node -r tsconfig-paths/register database/verify.ts"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "bcrypt": "^5.1.1",
    "winston": "^3.11.0",
    "pg": "^8.11.3",
    "typeorm": "^0.3.17",
    "reflect-metadata": "^0.1.13",
    "mongodb": "^6.3.0",
    "redis": "^4.6.12",
    "bullmq": "^5.3.0",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/bcrypt": "^5.0.2",
    "@types/pg": "^8.10.9",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  }
}
