{
  "name": "sandbox-service",
  "version": "1.0.0",
  "description": "Sandbox Integration Service",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "lint": "eslint src --ext .ts",
    "test": "jest --config tests/jest.config.ts",
    "test:unit": "jest --config tests/jest.config.ts --testPathPattern=unit",
    "test:integration": "jest --config tests/jest.config.ts --testPathPattern=integration",
    "test:watch": "jest --config tests/jest.config.ts --watch",
    "test:coverage": "jest --config tests/jest.config.ts --coverage",
    "test:ci": "jest --config tests/jest.config.ts --coverage --ci"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "axios": "^1.6.2",
    "ioredis": "^5.3.2",
    "bullmq": "^5.1.0",
    "file-type": "^18.7.0",
    "pdf-parse": "^1.1.1",
    "mammoth": "^1.6.0",
    "form-data": "^4.0.0",
    "typeorm": "^0.3.17",
    "reflect-metadata": "^0.1.13",
    "pg": "^8.11.3",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "@types/multer": "^1.4.11",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.0",
    "jest-mock-extended": "^3.0.5"
  }
}
