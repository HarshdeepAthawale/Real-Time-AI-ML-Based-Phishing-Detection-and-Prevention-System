{
  "name": "threat-intel",
  "version": "1.0.0",
  "description": "Threat Intelligence Service",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/index.ts",
    "lint": "eslint src --ext .ts",
    "test": "jest --config tests/jest.config.ts",
    "test:unit": "jest --config tests/jest.config.ts --testPathPattern=unit",
    "test:integration": "jest --config tests/jest.config.ts --testPathPattern=integration",
    "test:watch": "jest --config tests/jest.config.ts --watch",
    "test:coverage": "jest --config tests/jest.config.ts --coverage",
    "test:all": "jest --config tests/jest.config.ts --coverage --verbose",
    "test:setup": "bash scripts/setup-test-env.sh",
    "test:verify": "bash scripts/verify-test-env.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.2",
    "bloom-filters": "^1.3.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
